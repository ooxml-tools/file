{
  "name": "@ooxml-tools/file",
  "description": "Read/write Office Open XML files in nodejs/browser",
  "version": "0.4.0",
  "license": "MIT",
  "main": "./dist/npm/index.js",
  "types": "./dist/npm/types.d.ts",
  "type": "module",
  "bin": {
    "ooxml-file": "./dist/npm/bin/ooxml-file.js"
  },
  "scripts": {
    "lint": "npx prettier . --check",
    "test": "vitest",
    "test:ci": "vitest run --coverage --reporter=junit --outputFile=test-report.junit.xml",
    "lint:format": "npx prettier . --write",
    "build": "npm run build:data && npm run build:npm",
    "build:data": "tsx scripts/generate-blanks.ts",
    "build:npm": "rollup -c rollup.config.ts --configPlugin typescript"
  },
  "exports": {
    ".": "./dist/npm/index.js",
    "./commands": "./dist/npm/commands.js"
  },
  "files": [
    "./dist/npm",
    "./package.json",
    "./README.md"
  ],
  "devDependencies": {
    "@codecov/rollup-plugin": "^1.9.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@rollup/plugin-virtual": "^3.0.2",
    "@tsconfig/node22": "^22.0.0",
    "@types/yargs": "^17.0.32",
    "@vitest/coverage-v8": "^4.0.15",
    "prettier": "3.7.4",
    "rollup": "^4.18.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "rollup-plugin-typescript-paths": "^1.5.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "tsx": "^4.17.0",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=20.x"
  },
  "dependencies": {
    "glob": "^13.0.0",
    "jszip": "^3.10.1",
    "typescript": "^5.5.4",
    "yargs": "^18.0.0"
  }
}
